{
  "name": "cse_actemium",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode development",
    "build:development": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "fb:logout": "firebase logout",
    "fb:login": "firebase login",
    "fb:use:staging": "cross-env DEPLOY_ENV=staging env-cmd -f .env.staging node firebase-use.cjs",
    "fb:use:production": "cross-env DEPLOY_ENV=production env-cmd -f .env.production node firebase-use.cjs",
    "fb:deploy:staging": "cross-env env-cmd -f .env.staging node firebase-deploy.cjs",
    "fb:deploy:production": "cross-env env-cmd -f .env.production node firebase-deploy.cjs",
    "deploy:staging": "npm run build:staging && npm run fb:use:staging && npm run fb:deploy:staging",
    "deploy:production": "npm run build:production && npm run fb:use:production && npm run fb:deploy:production"
  },
  "dependencies": {
    "compress.js": "^1.2.2",
    "firebase": "^10.7.2",
    "framer-motion": "^11.0.3",
    "html-react-parser": "^5.1.1",
    "react": "^18.2.0",
    "react-cool-img": "^1.2.12",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^5.3.4",
    "react-slick": "^0.30.1",
    "react-toastify": "^10.0.4",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/compress.js": "^1.1.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
